<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="up_box_tittle" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="geminate_ecommerce_theme.s_3_tittle" t-thumbnail="/geminate_ecommerce_theme/static/src/img/extra_snippet/tittle/tittle_3.png"/>
            </xpath>
        </template>
        <template id="s_3_tittle" name="Tittle 3">
            <div class="s_gem_title_section_3">
                <div class="container">
                    <div class="gem_section-title_3">
                        <h5>Services</h5>
                        <h2> We Offer Best Services </h2>
                    </div>
                </div>
            </div>
        </template>
        <template id="tittle_btn3_snippet_options" inherit_id="website.snippet_options" name="tittle_btn3 snippet options">
            <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .s_gem_title_section_3</xpath>
        </template>
    </data>
</odoo>