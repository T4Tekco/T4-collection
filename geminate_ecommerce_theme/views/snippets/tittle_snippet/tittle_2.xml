<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="up_border_tittle" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="geminate_ecommerce_theme.s_2_tittle" t-thumbnail="/geminate_ecommerce_theme/static/src/img/extra_snippet/tittle/tittle_2.png"/>
            </xpath>
        </template>
        <template id="s_2_tittle" name="Tittle 2">
            <div class="s_gem_title_section_2">
                <div class="container">
                    <div class="section-title centered has-element">
                        <span class="subtitle">What We Provide</span>
                        <h3 class="title">Our<span class="color_change">Services</span></h3>
                    </div>
                </div>
            </div>
        </template>
        <template id="tittle_btn2_snippet_options" inherit_id="website.snippet_options" name="tittle_btn2 snippet options">
            <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .s_gem_title_section_2</xpath>
        </template>
    </data>
</odoo>