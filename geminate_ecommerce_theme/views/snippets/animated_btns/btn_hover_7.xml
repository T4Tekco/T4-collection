<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="btn_hvr_7" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="geminate_ecommerce_theme.s_7_hvr_btn" t-thumbnail="/geminate_ecommerce_theme/static/src/img/extra_snippet/btn/btn_main.png"/>
            </xpath>
        </template>
        <template id="s_7_hvr_btn" name="Btn 7">
            <div class="gem_hover_ef_1 gem_hover_ef_1_comn">
                <div class="conta iner">
                    <button class="btn-1_gem_24 main_btn_effect_comn main_btn_effect_1">Button 1</button>
                </div>
            </div>
        </template>
        <template id="ani_btn7_snippet_options" inherit_id="website.snippet_options" name="ani_btn7 snippet options">
            <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .gem_hover_ef_1, .gem_hover_ef_1_comn</xpath>
        </template>
    </data>
</odoo>
