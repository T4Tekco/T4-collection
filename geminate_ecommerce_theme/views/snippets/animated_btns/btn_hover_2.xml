<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="btn_hvr_2" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="geminate_ecommerce_theme.s_2_hvr_btn" t-thumbnail="/geminate_ecommerce_theme/static/src/img/extra_snippet/btn/btn_main.png"/>
            </xpath>
        </template>
        <template id="s_2_hvr_btn" name="Btn 2">
          <div class="gem_line_up_down_hover_ef">
                <div class="container">
                    <div class="col-sm-3">
                        <a href="#" class="btn_line_up_down btn_line_up_down-lg badge-primary">Button</a>
                    </div>
                </div>
            </div>
        </template>
        <template id="ani_btn2_snippet_options" inherit_id="website.snippet_options" name="ani_btn2 snippet options">
            <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .gem_line_up_down_hover_ef</xpath>
        </template>
    </data>
</odoo>
