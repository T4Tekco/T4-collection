<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="btn_hvr_8" inherit_id="website.snippets">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="geminate_ecommerce_theme.s_8_hvr_btn" t-thumbnail="/geminate_ecommerce_theme/static/src/img/extra_snippet/btn/btn_main.png"/>
            </xpath>
        </template>
        <template id="s_8_hvr_btn" name="Btn 8">
            <div class="gem_hover_ef_2 gem_hover_ef_1_comn">
                <div class="container">
                    <button class="btn-2_gem_24 main_btn_effect_comn main_btn_effect_2">Button 2</button>
                </div>
            </div>
        </template>
        <template id="ani_btn8_snippet_options" inherit_id="website.snippet_options" name="ani_btn8 snippet options">
            <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .gem_hover_ef_2, .gem_hover_ef_1_comn</xpath>
        </template>
    </data>
</odoo>
